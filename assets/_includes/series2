<div class = series>
  {% for tag in page.tags %}
  {% if tag contains ':' %}
    <ul>
    {% assign tgs = (site.tags | where_exp:'it', 'it[0] == tag') %}
    {% for tg in tgs %}
      {% assign page-list = (tg[1] | reverse) %}{% include page-list %}
    {% endfor %}
    </ul>
  {% endif %}
  {% endfor %}
</div>
